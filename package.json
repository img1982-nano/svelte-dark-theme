    "@jill64/sentry-sveltekit-cloudflare": "1.5.0",
    "@jill64/svelte-input": "1.5.2",
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-cloudflare": "2.3.4",
    "@sveltejs/kit": "1.27.7",
    "svelte": "4.2.8",
    "typescript": "5.3.3",
    "vite": "5.0.6"
  },
  "dependencies": {
    "@jill64/attempt": "1.1.0",
    "@jill64/svelte-device-theme": "1.2.4",
    "svelte-baked-cookie": "0.0.3",
    "@jill64/svelte-html": "1.0.0",
    "@jill64/svelte-storage": "0.0.5",
    "svelte-feather-icons": "4.0.1",
    "typescanner": "0.5.3"
  }
